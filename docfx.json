{
  "metadata": [
    {
      "src": "Structurizr.Core/**.cs",
      "dest": "docs/api/core",
      "disableGitFeatures": false
    },
    {
      "src": "Structurizr.Annotations/**.cs",
      "dest": "docs/api/annotations",
      "disableGitFeatures": false
    },
    {
      "src": "Structurizr.Reflection/**.cs",
      "dest": "docs/api/reflection",
      "disableGitFeatures": false
    },
    {
      "src": "Structurizr.Roslyn/**.cs",
      "dest": "docs/api/roslyn",
      "disableGitFeatures": false
    },
    {
      "src": "Structurizr.Cecil/**.cs",
      "dest": "docs/api/cecil",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/api/**.yml",
          "docs/api/index.md"
        ]
      },
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "docs/api/**",
          "docs/apidoc/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docs/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}